<script>
    function openEdit() {
        document.getElementById("tabEdit").style.display = "block";
    }
    function openDelete() {
        document.getElementById("tabDelete").style.display = "block";
    }
    var edit_tab = document.getElementById("tabEdit");
    var edit_del = document.getElementById("tabDelete");
    window.onclick = function(event) {
        if(event.target === edit_tab) {
            edit_tab.style.display = "none";
        }
        if(event.target === edit_del) {
            edit_del.style.display = "none";
        }
    }
</script>
